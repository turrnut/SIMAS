@ To see how many times myFunction has been called;
@ Should output 5;

SET NUM counter 0;

FUN myFunction 0;
    PRINTLN;
    ADD NUM counter 1;
    PRINTC myFunction called ;
    PRINT counter;
    PRINTC  times.;
    RET;
END FUN;

CALL myFunction;
CALL myFunction;
CALL myFunction;
CALL myFunction;
CALL myFunction;

